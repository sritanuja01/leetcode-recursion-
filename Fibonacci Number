class Solution {
    public int fib1(int a,int b,int n)
    {
        int c=0;
        c=a+b;
        n--;
        if(n<=0)
        {
            return b;
        }
        return fib1(b,c,n);
        
    }
    public int fib(int n) {
        int a=0,b=1;
        if(n==0)
        {
            return 0;
        }
        return fib1(a,b,n);
    }
}
